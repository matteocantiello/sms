&star_job

   show_log_description_at_start = .false.
   
   load_saved_model = .true.
   load_model_filename = 'zams_1d2Msun.mod'      ! from make_zams_ultra_high_mass, 13830.
   
	
   save_model_when_terminate = .true.
   save_model_filename = 'tams_1d2Msun.mod'
   required_termination_code_string = 'xa_central_lower_limit'

   set_initial_cumulative_energy_error = .true.
   new_cumulative_energy_error = 0d0

   pgstar_flag = .true.
   save_pgstar_files_when_terminate = .true.

/ ! end of star_job namelist

&eos
/

&kap
   Zbase = 1d-5
/ ! end of kap namelist

&controls

   ! To stop on preMS
   ! abs_diff_lg_LH_lg_Ls_limit = 0.01
  
   ! Include TOV correction  
   use_other_cgrav = .true.

   ! Use custom accretion routine   
   use_other_accreting_state = .true. 
  

   ! Set Accretion properties (Important: Accretio Rate x_ctrl(1) should Match what is set in mass_change. Probably will automate this later)
   x_ctrl(1) = 0.1    ! Accretion Rate (Msun/yr), to calculate Pressure and Density of the accretio stream
   x_ctrl(2) = 5000   ! Temperature of Accreted Material (K)
   mass_change = 0.1  ! Accretion Rate (Msun/yr), to change stellar mass


   ! Treatment of radiation-dominated envelopes 
   Pextra_factor = 2.0  ! 1.36                 ! Extra Pressure at surface (See Herrington et al. https://arxiv.org/pdf/2208.00008 )  
   okay_to_reduce_gradT_excess = .true.        ! Reduction of Superadiabaticity  
   opacity_max = 1                             ! Set limits on opacity (this is an artificial / engineering trick to avoid dealing with outer envelope stability issues) 


   ! Energy Conservation 
   ! energy_eqn_option = 'dedt'
   num_trace_history_values = 2
   trace_history_value_name(1) = 'rel_E_err'
   trace_history_value_name(2) = 'log_rel_run_E_err'


   ! Initial Conditions 
   initial_mass = 100
   initial_z = 1d-5


   ! Convection
   mixing_length_alpha = 2
   conv_core_gap_dq_limit = 0.1

   !xa_central_lower_limit_species(1) = 'h1'
   !xa_central_lower_limit(1) = 0.05

  ! overshoot_scheme(1) = 'exponential'
  ! overshoot_zone_type(1) = 'nonburn'
  ! overshoot_zone_loc(1) = 'shell'
  ! overshoot_bdy_loc(1) = 'bottom'
  ! overshoot_f(1) = 0.128
  ! overshoot_f0(1) = 0.100

  ! overshoot_scheme(2) = 'exponential'
  ! overshoot_zone_type(2) = 'any'
  ! overshoot_zone_loc(2) = 'any'
  ! overshoot_bdy_loc(2) = 'any'
  ! overshoot_f(2) = 0.014
  ! overshoot_f0(2) = 0.004

   ! Convergence and tolerances
   mesh_delta_coeff = 2.0d0  ! 1.0
   varcontrol_target = 1d-3
   ! use_gold2_tolerances = .true.
  
  
   ! Output 
   photo_interval = 50
   profile_interval = 100
   history_interval = 10
   terminal_interval = 10
   write_header_frequency = 100

/ ! end of controls namelist
